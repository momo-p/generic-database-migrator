{
  "name": "generic-database-migrator",
  "version": "0.0.3",
  "description": "Low level framework for database migration.",
  "repository": {
    "type": "git",
    "url": "git://github.com/momo-p/generic-database-migrator.git"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.js",
  "files": [
    "/dist/src"
  ],
  "scripts": {
    "test": "ts-mocha test/**/*.test.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "pretty": "prettier \"./src/**/*.ts\" \"./test/**/*.ts\" --write",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "keywords": [
    "database",
    "migration",
    "migrator",
    "synchronizer",
    "sql"
  ],
  "author": "momo-p <contact@relay.akimitsu.dev> (https://github.com/momo-p)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/async-lock": "^1.4.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "async-lock": "^1.4.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^9.0.0",
    "lint-staged": "^13.2.2",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "src/**/*.{js,json,ts,xml,yaml}": [
      "pnpm lint",
      "prettier --write"
    ],
    "test/**/*.{js,json,ts,xml,yaml}": [
      "pnpm lint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@supercharge/promise-pool": "^2.4.0"
  }
}
